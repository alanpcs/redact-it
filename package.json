{
  "name": "@alanpcs/redact-it",
  "version": "0.1.1",
  "description": "A flexible tool to redact JS objects' data",
  "author": "AlanPCS <alanpcsilva+npm@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@eslint/js": "^9.28.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.29",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "chai": "^5.2.0",
    "eslint": "^9.28.0",
    "jiti": "^2.4.2",
    "karma-nyan-reporter": "^0.2.5",
    "mocha": "^11.5.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "rollup": "^4.41.1",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "test": "run-p lint test:compile test:mocha",
    "test:ci": "run-p lint test:compile test:coverage",
    "test:compile": "tsc",
    "test:mocha": "ts-mocha --config test/.mocharc.json",
    "test:watch": "yarn test:mocha --watch -R min",
    "test:coverage": "nyc --reporter=text yarn test:mocha -R spec",
    "lint": "eslint --config .eslintrc.config.ts 'src/**/*.ts' 'test/**/*.test.ts'",
    "lint:fix": "yarn lint --fix",
    "build": "rm -rf dist && rollup -c rollup.config.ts --bundleConfigAsCjs && tsc --emitDeclarationOnly",
    "deploy": "yarn build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/alanpcs/redact-it.git"
  }
}
