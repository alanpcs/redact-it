{
  "name": "@alanpcs/redact-it",
  "version": "0.1.1",
  "description": "A flexible tool to redact JS objects' data",
  "author": "AlanPCS <alanpcsilva+npm@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@eslint/js": "^9.38.0",
    "@jest/globals": "^30.2",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.2.0",
    "@types/chai": "^5.2.3",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.29",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-jest": "^30.2",
    "eslint": "^9.38.0",
    "eslint-plugin-jest": "^29.0.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "karma": "^6.4.4",
    "karma-nyan-reporter": "^0.2.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tslint": "^6.1.3",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "run-p lint test:compile test:coverage",
    "test:compile": "tsc",
    "test:mocha": "mocha --config test/.mocharc.json",
    "test:watch": "yarn test:mocha --watch -R min",
    "test:coverage": "nyc --reporter=text yarn test:mocha -R spec",
    "lint": "eslint --config .eslintrc.config.ts 'src/**/*.ts' 'src/**/*.test.ts'",
    "lint:fix": "yarn lint --fix",
    "build": "rm -rf dist && rollup -c rollup.config.ts --bundleConfigAsCjs && tsc --emitDeclarationOnly",
    "deploy": "yarn build && npm publish",
    "bless": "yarn tsc --noEmit && yarn lint && yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/alanpcs/redact-it.git"
  },
  "engines": {
    "node": "22"
  }
}
